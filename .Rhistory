library(epinionDSB)
mtcars2 <- mtcars %>%
tibble::rownames_to_column("car") %>%
mutate(carb2 = ifelse(car == "Mazda RX4",
10,
carb)) %>%
mutate(carb3 = ifelse(car == "Volvo 142E",
12,
carb2))
# it only works with exact match of length of palette and number of categories
# should be able to just pick what it needs
ggplot(mtcars2, aes(x = wt,
y = mpg,
color = factor(carb3))) +
geom_point(size = 7) +
facet_wrap(~ vs) +
dsb_style() +
color_dsb_d()
table(mtcars$cyl)
ggplot(mtcars, aes(x = wt,
y = mpg,
color = factor(am))) +
geom_point(size = 5) +
facet_wrap(~ vs) +
dsb_style()
# ... all DSB colors
dsb_colvec()
# a selection of colors
dsb_colvec("DSB_Red", "DSB_DarkBlue")
ggplot(mtcars, aes(x = wt,
y = mpg)) +
geom_point(size = 5,
color = getElement(dsb_colvec(), "DSB_Red")) +
facet_wrap(~ vs) +
dsb_style()
ggplot(mtcars, aes(x = wt,
y = mpg,
color = factor(am))) +
geom_point(size = 5) +
facet_wrap(~ vs) +
dsb_style() +
scale_colour_manual(values = c(getElement(dsb_colvec(), "DSB_Red"),
getElement(dsb_colvec(), "DSB_DarkBlue"))) +
theme(legend.position = "none")
